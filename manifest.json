{
  "name": "adb",
  "version": "0.1",
  "description": "Ad Blocker",
  "update_url": "http://www.moreslowly.jp/chrome/extensions/adb/updates.xml",
  "content_scripts": [ {
      "js": [ "adb.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
  } ],
  "background_page": "background.html",
  "options_page": "options.html",
  "permissions": [ "http://*/", "https://*/*", "tabs" ]
}
